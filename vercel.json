{
    "version": 2,
    "rewrites": [
        {
            "source": "/api/admin/:path*",
            "destination": "https://productionbackend-3sr4.onrender.com/api/admin/:path*"
        },
        {
            "source": "/api/:path*",
            "destination": "https://productionbackend-3sr4.onrender.com/api/:path*"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "headers": [
        {
            "source": "/api/admin/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, s-maxage=300, stale-while-revalidate=60"
                }
            ]
        }
    ]
}